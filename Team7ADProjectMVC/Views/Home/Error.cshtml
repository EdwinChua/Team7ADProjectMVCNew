@{
    Layout = "~/_Layout1.cshtml";
    Employee currentEmployee = (Employee)Session["User"];
    Page.Employee = currentEmployee;
    Page.Title = "Oops! We seem to have encountered a problem!";
    var message = Html.ViewContext.HttpContext.Request.QueryString.Get("message");
}
<script type="text/javascript" src="~/Resources/_error.js"></script>
<link rel="stylesheet" href="~/Resources/errorpage.css" />
<div class="container-fluid">
    <div id="parallax_wrapper">
        <div id="parallax_field">
            <img alt="" class="js-plaxify" data-invert="true" data-xrange="0" data-yrange="20" height="415" id="parallax_bg" width="940" src="~/Resources/Error_Page/main.jpg" style="top: -19.7264px; left: -20px;">
        </div>
        <div id="parallax_illustration">
            <div id="auth"></div>
            <img alt="404 “This is not the web page you are looking for”" class="js-plaxify" data-xrange="20" data-yrange="10" height="249" id="parallax_error_text" width="271" src="~/Resources/Error_Page/NotWhatYouRLookingFor.png" style="top: 71.8632px; left: 75.3579px;">
            <img alt="" class="js-plaxify" data-xrange="10" data-yrange="10" height="230" id="parallax_octocat" width="188" src="~/Resources/Error_Page/jedi.png" style="top: 93.8632px; left: 357.679px;">
            <img alt="" class="js-plaxify" data-xrange="10" data-yrange="10" height="156" id="parallax_speeder" width="440" src="~/Resources/Error_Page/spacecraft.png" style="top: 149.863px; left: 433.679px;">
            <img alt="" class="js-plaxify" data-xrange="10" data-yrange="10" height="49" id="parallax_octocatshadow" width="166" src="~/Resources/Error_Page/oval.png" style="top: 296.863px; left: 372.679px;">
            <img alt="" class="js-plaxify" data-xrange="10" data-yrange="10" height="75" id="parallax_speedershadow" width="430" src="~/Resources/Error_Page/base.png" style="top: 262.863px; left: 443.679px;">
            <img alt="" class="js-plaxify" data-invert="true" data-xrange="50" data-yrange="20" height="123" id="parallax_building_1" width="304" src="~/Resources/Error_Page/hut.png" style="top: 73.2736px; left: 458.605px;">
            <img alt="" class="js-plaxify" data-invert="true" data-xrange="75" data-yrange="30" height="50" id="parallax_building_2" width="116" src="~/Resources/Error_Page/smallhut.png" style="top: 113.41px; left: 748.908px;">
        </div>
    </div>


    <h4></h4>
    <div class="jumbotron">
        Please try again. If you encounter the same problem, please contact Team 7 for technical assistance, and quote the following error:
        <br /><br />
        <div class="alert alert-danger">
            Server error: @message
        </div>
    </div>
    <br />
</div>


